{
  "name": "cyptest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:reset": "sh reset-test.sh",
    "dev": "next dev",
    "build": "next build",
    "build:test": "npm run db:reset && set APP_ENV=test&& set NODE_ENV=test&& next build",
    "start:test": "set APP_ENV=test&& set NODE_ENV=test&& next start",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress:open": "env-cmd -f .env.test.local cypress open",
    "cypress:start": "start-server-and-test 'npm run start:test' 3000 'npm run cypress:open'",
    "cypress:build": "npm run build:test && npm run cypress:start"
  },
  "dependencies": {
    "next": "^14.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "undici": "^5.28.4"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cypress": "^13.8.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "^14.2.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.2.13",
    "postcss": "^8",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "description": "This is a [Next.js](https://nextjs.org/) project bootstrapped with [`create-next-app`](https://github.com/vercel/next.js/tree/canary/packages/create-next-app).",
  "main": "jest.polyfills.js",
  "author": "",
  "license": "ISC"
}
